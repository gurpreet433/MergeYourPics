<?xml version="1.0" encoding="utf-8"?>

<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item
        android:width="25dp"
        android:height="25dp">
        <shape android:shape="rectangle">
            <solid android:color="@color/colorAccent"/>
        </shape>
    </item>

    <item
        android:width="7dp"
        android:height="2dp"
        android:left="4dp"
        android:top="15dp">
        <rotate
            android:fromDegrees="45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>

    <item
        android:width="15dp"
        android:height="2dp"
        android:top="13dp"
        android:left="7dp">
        <rotate
            android:fromDegrees="-45">
            <shape android:shape="rectangle">
                <solid android:color="@color/white"/>
            </shape>
        </rotate>
    </item>
</layer-list>